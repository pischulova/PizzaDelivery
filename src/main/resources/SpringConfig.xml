<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
          http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context-4.1.xsd
           http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean name="pizzaRepositoryTest" class="ua.epam.rd.repository.PizzaRepositoryTest"/>

    <bean name="orderRepositoryTest" class="ua.epam.rd.repository.OrderRepositoryTest"/>

    <bean name="pizzaServiceTest" class="ua.epam.rd.service.PizzaServiceTest">
        <constructor-arg ref="pizzaRepositoryTest"/>
    </bean>

    <bean name="orderServiceTest" class="ua.epam.rd.service.OrderServiceTest">
        <constructor-arg ref="orderRepositoryTest"/>
    </bean>



</beans>
